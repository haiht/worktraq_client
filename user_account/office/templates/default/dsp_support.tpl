<section id="main">
    <ul class="breadcrumb">
        <li><a href="#" title="Home">Home</a><span class="divider">/</span></li>
        <li class="active">Support </li>
    </ul>

    <h2 class="title-page"><span>Support</span></h2>
    <section id="content">
        <div class="wrap-content">
            <h3>Contact Infomation</h3>
            <div class="context contact-info">
                [@CONTACT_INFO]
            </div>
            <h3>Feedback</h3>
            <div class="context">
                [@EROR_RESULT]
                <br>
                <form id="report-error" name="report-error" method="post" action="[@ACTION]" class="form-general report-error">
                    <div>
                        <label for="txt_support_title">Subject:</label>
                        <input type="text" id="txt_support_title" name="txt_support_title"  value=""/>
                    </div>
                    <div>
                        <label for="txt_support_description">Your Comments:</label>
                        <textarea id="txt_support_description"  name="txt_support_description" ></textarea>
                    </div>
                    <div>
                        <input type="submit" id="btn_submit_tb_product" name="btn_submit_tb_product" class="btn btn-primary" value="Send your Feedback"/>
                    </div>
                </form>
            </div>
            <h3>Live Support Chat</h3>
            <div class="context">
                <p>Coming soon</p>
            </div>
            <h3>Frequently Asked Questions</h3>
            <div class="context">
                <p>Coming soon</p>
            </div>
        </div>
    </section>
</section>