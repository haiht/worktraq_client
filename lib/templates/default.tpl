body {
    color: [@FONT_COLOR];
}
#top_page ul li a, #page .bottom_slide a.about, .header .sub a.sub_active, #page .float_right .device_page ul li a, #page .content .device_page ul li a, #page .float_right .device_page a.view {
    color: [@NAVIGATOR_LINK_COLOR];
}
#top_page ul li a:hover, #page .bottom_slide a.about:hover, #page .bottom_slide a.about:active, #page .bottom_slide a.about:visited, .header .sub a.sub_active:hover, #page .float_right .device_page ul li a:hover, #page .float_right .device_page a.view:hover{
    color: [@NAVIGATOR_LINK_COLOR_HOVER];
}
#page .content .right_ct .prod_1 div.box a{
	color:[@INFOMATION_ORDER_COLOR]
}
#page .content .right_ct .prod_1 div.box a:hover{
