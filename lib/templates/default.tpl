	color:[@BUTTON_ORDER_DETAILS_COLOR];
}
#page .content .float_right .indent div.reate{
	border-color: [@BACKGROUND_ORDER_DEATILS_COLOR];
}

.content .float_right .indent .box_2 .infor a{
	background:[BORDER_BUTTON_ORDER_DETAILS];
