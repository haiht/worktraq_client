	color:[@TITLE_PRODUCT_DETAILS_COLOR];
}

#page .content .right_ct .prod_1 .title_prod{
	color:[@TITLE_PRODUCT_IN_CATALOGUE_COLOR];
}
#page .content .left_ct .from_left_ct{
	color:[@LIST_MENU_LEFT_COLOR];
}
.header .sub a{
