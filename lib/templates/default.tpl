}
ul.nav{
	background:[@BACKGROUND_MENU_COLOR] !important;
}
ul.nav li a:hover, ul.nav li a.active {
    background: none repeat scroll 0 0 [@BACKGROUND_MENU_COLOR_HOVER];
}

#page .content .float_right .title_r, #page .content .left_ct .from_left_ct .first, #page .content .right_ct .prod_1 .title_prod{
